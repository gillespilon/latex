@entry{randomeffectsmodel,
name = {random effects model},
plural = {random effects models},
description = {It is also called a variance components model. It is a statistical model where the model parameters are random quantities. The mathematical model that describes the relationship between the response and treatment for the one-way ANOVA is given by $Y_{ij} = \mu + \tau + \epsilon_{ij}$, where $Y_{ij}$ represents the j\textsuperscript{th} \gls{observation} ($j = 1, 2, \ldots, n_i$) on the i\textsuperscript{th} treatment ($i = 1, 2, \ldots, k$ levels). $\mu$ is the common effect for the whole \gls{experiment}, $\tau_i$ represents the i\textsuperscript{th} treatment effect, and $\epsilon_{ij}$ represents the random error present in the j\textsuperscript{th} \gls{observation} on the i\textsuperscript{th} treatment. The errors $\epsilon_{ij}$ are assumed to be normally and independently (NID) distributed, with mean zero and variance $\sigma^2_\epsilon$. $\mu$ is always a fixed parameter, and $\tau_1, \tau_2, \ldots, \tau_k$ are considered to be random parameters if the levels of the treatment are random and not a fixed sample from a population of possible levels. It is also assumed that $\mu$ is chosen so that $\sum \tau_i = 0,\;i = 1, \ldots, k$ holds. This is the random effects model.}
}
