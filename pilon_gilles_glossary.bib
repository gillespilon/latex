}
@entry{applicationprogramminginterface,
name = {application programming interface},
plural = {application programming interfaces},
description = {It is a way for two or more \glspl{computerprogram} to communicate with each other. It is a type of software interface, offering a service to other pieces of software.}
}
@entry{argument,
name = {argument},
plural = {arguments},
description = {Arguments are the values assigned to \glspl{parameter} when passed into a Python function or method during a function call.}
}
@entry{argumentpositional,
name = {positional argument},
plural = {positional arguments},
description = {Positional arguments are values that are passed into a function based on the order in which the \glspl{parameter} were listed during the function definition.}
}
@entry{argumentkeyword,
name = {keyword argument},
plural = {keyword arguments},
description = {Keyword arguments (named arguments) are values that, when passed into a function, are identifiable by specific \gls{parameter} names. A keyword argument is preceded by a \gls{parameter} and the assignment operator, =.}
}
@entry{artificialintelligence,
name = {artificial intelligence},
description = {Artificial intelligence (AI) is about creating computer systems that can perform tasks that typically require human intelligence, such as understanding language, recognizing images, and making decisions. It as teaching computers to think and learn like humans.}
}
@entry{artificialnarrowintelligence,
name = {artificial narrow intelligence},
description = {Narrow artificial intelligence is limited to a single, narrowly defined task. Most modern \gls{artificialintelligence} systems are of this type. It is also called weak artificial intelligence.}
}
@entry{artificialgeneralintelligence,
name = {artificial general intelligence},
description = {Artificial general intelligence (AGI) means machines will be endowed with intelligence as good as the smartest human in any field, but because of its capabilities to integrate learning across many fields, it will probably become better than any average doctor, lawyer or computer programmer.}
}
@entry{artificialsuperintelligence,
name = {artificial super intelligence},
description = {Artificial super intelligence (ASI) is a computer brain that can exceed what any human can do in any field and then, with its \gls{polymath}ic ability, it could produce insights far beyond anything humans could do or even imagine. It might even invent its own language we do not understand.}
}
@entry{averagechart,
plural = {average charts},
name = {average chart ($\overline{X}$)},
description = {It is a process behaviour chart that plots the \gls{averagestatistic} of a subgroup of values taken in a rational manner. The chart also has an upper \gls{controlchartlimit} line, a lower \gls{controlchartlimit} line, and an \gls{averagestatistic} line.}
}
@entry{averagestatistic,
plural = {averages},
name = {average},
description = {It is the sum of a set of values divided by the count of values. It is a type of \gls{locationstatistic}. $$\overline{Y} = \frac{\sum\limits_{i=1}^{n} X_i}{n}$$}
}
@entry{averagerangestatistic,
plural = {average ranges},
name = {average range},
description = {It is the \gls{averagestatistic} of several \glspl{rangestatistic} of sample subgroups. A subgroup is sampled, a \gls{rangestatistic} is calculated, and the \gls{averagestatistic} of the \glspl{rangestatistic} is calculated.}
}
@entry{beautifulsoup,
name = {Beautiful Soup},
description = {It parses HTML and XML documents to create a tree-like representation
of the document structure and extract \glspl{datum}. (\cite{beautifulsoup})}
}
@entry{betweenoperatorerror,
name = {between-operator error},
plural = {between-operator errors},
description = {It is the variation in the \gls{averagestatistic} of measurements made by different operators measuring one characteristic on the same parts. It can be caused by differences in training, eduction, skill, and care in taking the measurements.}
}
@entry{betweenparterror,
name = {between-part error},
plural = {between-part errors},
description = {It is the variation of measurements between samples of parts, that is, the part-to-part variation. If a sample is carefully chosen, the error is shown on the \gls{individualschart} or the \gls{averagechart}.}
}
@entry{betweensubgrouperror,
plural = {between-subgroup errors},
name = {between-subgroup error},
description = {The subgroup averages are used to find an indirect estimate of the \gls{standarddeviation} of X.}
}
@entry{biaslinearity,
name = {bias and linearity},
description = {This is the difference between the measurement result and its unknown, true values. It can be eliminated by calibration to a standard. Bias is an \gls{averagestatistic} over all references values. Linearity or non-linearity is a difference in bias across all reference values.}
}
@entry{biometricdevice,
name = {biometric device},
plural = {biometric devices},
description = {A biometric device is an electronic device that uses biometric identifiers to identify and verify individuals.}
}
@entry{biometricidentifier,
name = {biometric identifier},
plural = {biometric identifiers},
description = {A biometric identifier is a unique physical characteristic that can be used to identify an individual. Examples include fingerprint, retina or iris, voiceprint, hand geometry, face geometry, ear geometry, and heartbeat.}
}
@entry{blocking,
name = {blocking},
description = {It is groups of experimental runs in which randomisation is restricted within a group, for example, four runs in the morning and four runs in the afternoon. The relative homogeneity within a block and the identification and inclusion of the block as a factor in the \gls{experiment} can be used to increase the accuracy of an \gls{experiment}.}
}
