}
@entry{controlchart,
plural = {control charts},
name = {control chart},
description = {It is a graphical plot used to determine if process variation is stable and predictable. A process that is in \gls{statisticalcontrol} means that the variation is due to \glspl{commoncause}. A process that is not in \gls{statisticalcontrol}, that is, out of control, means that the variation is due to \glspl{specialcause}.},
}
@entry{controlchartlimit,
plural = {control chart limits},
name = {control chart limit},
description = {This is a limit of predictability for the \glspl{datum} plotted on the relevant control chart. It is usually set at three \glspl{standarddeviation} from a measure of \gls{location}.}
}
@entry{conversationalartificialintelligence,
name = {conversational artificial intelligence},
description = {Conversational artificial intelligence refers to any \gls{artificialintelligence} system designed to interact with users through \gls{naturallanguage} conversation.}
}
@entry{datum,
name = {datum},
plural = {data},
description = {A datum is an individual value in a collection of data, discrete or continuous values that convey information, describing the quantity, quality, fact, \gls{statistic}, other basic units of meaning, or simply sequences of symbols that may be further interpreted formally. Data are usually organized into structures such as tables that provide additional context and meaning, and may themselves be used as data in larger structures.}
}
@entry{dataanalyst,
name = {data analyst},
plural = {data analysts},
description = {This individual can quickly get a sense of what is worth pursuing with more care in an existing \gls{dataset}. Fast and humble, and sloppy code are acceptable traits of this role. This individual uses \gls{statistic} and visualizations to explore the \glspl{datum}, without coming to conclusions or decisions.}
}
@entry{dataengineer,
name = {data engineer},
plural = {data engineers},
description = {A data engineer prepares \glspl{datum} for analytical and operational uses, that is, \gls{etl}. This individual is capable of preparing \glspl{datum} at scale from multiple sources and making it available to use in a production environment.}
}
@entry{dataleakage,
name = {dataleakage},
description = {In \gls{machinelearning}, it is learning something from the \gls{testdataset} that one is not allowed to know.}
}
@entry{datascience,
name = {data science},
description = {It is the science of extracting knowledge and insights from \glspl{datum}.}
}
@entry{datasciencearchitect,
name = {data science architect},
plural = {data science architects},
description = {The \gls{datascience} architect designs the collection, storage, and analyses processes, while considering time and cost-tradeoffs, and business requirements. This individual has expertise as a \gls{dataengineer}, \gls{dataanalyst}, \gls{machinelearningengineer}, and \gls{statistician}.}
}
@entry{datascientist,
name = {data scientist},
plural = {data scientists},
description = {A data scientist applies the methodologies and tools of \gls{datascience}. This individual has expertise as a \gls{dataanalyst}, \gls{machinelearningengineer}, and \gls{statistician}.}
}
@entry{dataset,
name = {data set},
plural = {data sets},
description = {A data set is a collection of \glspl{datum}. In the case of tabular \glspl{datum}, a data set corresponds to one or more database tables, where every column of a table represents a particular variable, and each row corresponds to a given record of the data set in question. The data set lists values for each of the variables, such as for example height and weight of an object, for each member of the data set. Data sets can also consist of a collection of documents or files.}
}
@entry{dawgdad,
name = {dawgdad},
description = {It is a Python package for statistical, graphical, and predictive analysis. To use dawgdad, install a Python distribution appropriate to your operating system. Then install dawgdad following the instructions at \href{https://github.com/gillespilon/dawgdad}{dawgdad}.}
}
@entry{decisionmaker,
name = {decision maker},
plural = {decision makers},
description = {It is someone who understands the art and science of data-driven decision-making. This individual is responsible for identifying decisions worth making with \glspl{datum}, framing them (everything from designing metrics to calling the shots on statistical assumptions), and determining the required level of analytical rigour based on potential impact on the business.}
}
@entry{deeplearning,
