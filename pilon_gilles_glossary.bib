description = {It is a process that aims to improve an entity by triggering significant changes to its properties through combinations of information, computing, communication, and connectivity technologies.}
}
@entry{discriminativemodel,
plural = {discriminative model},
name = {discriminative models},
description = {In \gls{statisticalclassification} a discriminative approach computers a classifier such as logistic regression. A discriminative model is a model of the \gls{conditionalprobability} $P(Y | X = x)$ of the target $Y$ given an \gls{observation} $x$. It can be used to discriminate the value of the target variable $Y$ given an \gls{observation} $x$.}
}
@entry{dispersion,
plural = {dispersions},
name = {dispersion},
description = {A sample of values will not (usually) have all values identical. Dispersion is the variation between values. Large dispersion means the values are widely scattered. Small dispersion means the values are narrowly scattered.}
}
@entry{dispersionstatistic,
plural = {dispersion statistics},
name = {dispersion statistic},
description = {It is a statistic that estimates the spread or variability of the \glspl{datum}. It is also called a measure of scale. Examples include the variance, the \gls{standarddeviation}, the \gls{rangestatistic}, the \gls{aad}, the \gls{mad}, and the \gls{iqr}.}
}
@entry{distancetonearestspecification,
name = {distance to nearest specification (sigma level)},
description = {Is the difference between the \gls{location} statistic and the closest specification limit, divided by the \gls{dispersion} statistic (e.g. the \gls{standarddeviation}).}
}
@entry{distancetotarget,
name = {distance to target},
description = {The difference between the specification target and the \gls{averagestatistic} is the difference you must adjust to aim the process correctly.}
}
@entry{distributionpackage,
name = {distribution package},
plural = {distribution packages},
description = {A distribution package is a version controlled archive file that contains Python packages, modules, and other resources files that are used to distribute a release. The archive file is what an end-user will download from the internet and install. A distribution package is a piece of software that you can install. Most of the time, this is synonymous with “project.” When you type \textcolor{orange}{pip install pkg}, or when you write \textcolor{orange}{dependencies = [\enquote{pkg}]} in your \textcolor{orange}{pyproject.toml}, pkg is the name of a distribution package. When you search or browse the \href{https://pypi.org/}{PyPI}, the most widely known centralized source for installing Python libraries and tools, what you see is a list of distribution packages.}
}
@entry{docstring,
name = {docstring},
plural = {docstrings},
description = {It is the embedded documentation for a Python module, class, function etc., usually in code as a string at the beginning of the object's definition.}
}
@entry{dotplot,
