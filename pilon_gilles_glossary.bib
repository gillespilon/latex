@entry{deprecation,
name = {deprecation},
plural = {deprecations},
description = {A document has been superseded. It is recommended to not use the older document. It may not conform to current standard. It may contain errors or incorrect information. It may contain links to documents that no longer exist or their location is unknown. The content may be ambiguous or confusing to users.}
}
@entry{depreciation,
name = {depreciation},
description = {It is a business expense. At some point a physical asset needs repairs or becomes obsolete. Money is spent before one gets revenue. A business should typically spend more than its depreciation to repair or acquire physical assets. Depreciation spreads out the cost of a tangible asset over its useful life. It is a non cash charge to account for the loss of value of a tangible assets over time.}
}
@entry{designedexperiment,
