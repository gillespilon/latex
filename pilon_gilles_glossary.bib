@entry{hiddenlayer,
name = {hidden layer},
plural = {hidden layers},
description = {It is a synthetic layer in a \gls{artificialneuralnetwork} between the input layer (the \glspl{exog}) and the output layer (the \gls{prediction}). Hidden layers typically contain an activation function (such as ReLU) for training. A \gls{deepneuralnetwork} contains more than one hidden layer.}
}
@entry{hubris,
