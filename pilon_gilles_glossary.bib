}
@entry{ebitda,
name = {EBITDA},
description = {\Gls{earnings} Before \Gls{interest} \Gls{taxes} \Gls{depreciation} and \Gls{amortization}. If the EBITDA is not stated, one can calculate EBIT from earnings (net income) + non operating expense + income taxes on the income statement, and DA from non cash charges on the cash flow statement. EBITDA is typically used by private equity companies to inflate their cash flow in order to obtain loans for business acquisitions.}
}
@entry{edr,
name = {endpoint detection and response},
description = {Endpoint detection and response (EDR) is \gls{cybersecurity} technology that continually monitors an endpoint (mobile phone, laptop, internet of things devices) to mitigate malicious cyber threats},
}
@entry{etl,
name = {extract, transform, and load},
description = {At a high level extract, transform, and load  (ETL) extracts, transforms, and loads \glspl{datum}. The specific tasks include: building \glspl{datum} pipelines to gather information from difference source systems; integrate, consolidate, and clean data; structure the \glspl{datum} for use in individual analytics applications.}
}
