@entry{effect,
name = {effect},
plural = {effects},
description = {It is the change in response caused by a change in the level of a factor (\cite[][114]{hicks1982}).}
}
@entry{embeddedsystem,
name = {embedded system},
plural = {embedded systems},
description = {An embedded system is a specialized computer system; a combination of a computer processor, computer memory, and peripheral devices for input and output, that has a dedicated function within a larger mechanical or electronic system. It is embedded as part of a complete device often including electrical or electronic hardware and mechanical parts. Because an embedded system typically controls physical operations of the machine that it is embedded within, it often has real-time computing constraints. Embedded systems control many devices in common use.}
}
@entry{embeddedoperatingsystem,
name = {embedded operating system},
plural = {embedded operating systems},
description = {An embedded operating system is an \gls{operatingsystem} designed and optimized to improve the efficiency of managing the hardware resources and reduce response times specifically for the task the device was created for.}
}
@entry{empiricalmethod,
name = {empirical method},
description = {It is any procedure for conducting an investigation that relies upon experimentation and systematic \gls{observation} rather than theoretical speculation.}
}
@entry{endog,
name = {endogenous variable},
plural = {endogenous variables},
description = {Endogenous variables are explained or predicted by a model. They are caused by factors within the system. Other names are y, dependent variable, regressand, outcome, response variable, and target (\gls{machinelearning}). In $y = mX + b$, y is the endogenous variable.}
}
@entry{exog,
name = {exogenous variable},
plural = {exogenous variables},
description = {Exogenous variables are terms in a model. They are caused by factors outside the system. Other names are x, independent variable, regressor, design, explanatory variable, and feature (\gls{machinelearning}). In $y = mX + b$, x is the exogenous variable. In a \gls{designedexperiment}, it is a factor that you can control, want to control, or is controlled at low cost.}
}
@entry{experiment,
name = {experiment},
plural = {experiments},
description = {It is a series of \glspl{observation} conducted under controlled conditions to study a relationship with the purpose of drawing causal inferences about that relationship. An experiment involves the manipulation of an independent variable, the measurement of a dependent variable, and the exposure of various participants to one or more of the conditions being studied. Random selection of participants and their random assignment to conditions also are necessary in experiments.}
}
@entry{expectedmeansquares,
name = {expected mean squares},
description = {They are the expected values of certain \gls{statistic} arising in partitions of sums of squares in the analysis of variance. They can be used for ascertaining which statistic should appear in the denominator in an F test for testing a null hypothesis that a particular effect is absent.}
}
