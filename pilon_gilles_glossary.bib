@entry{endog,
name = {endogenous variable},
plural = {endogenous variables},
description = {Endogenous variables are explained or predicted by a model. They are caused by factors within the system. Other names are y, dependent variable, regressand, outcome, response variable, and target (machine learning). In $y = mX + b$, y is the endogenous variable.}
}
@entry{exog,
name = {exogenous variable},
plural = {exogenous variables},
description = {Exogenous variables are terms in a model. They are caused by factors outside the system. Other names are x, independent variable, regressor, design, explanatory variable, and feature (machine learning). In $y = mX + b$, x is the exogenous variable. In a \gls{designedexperiment}, it is a factor that you can control, want to control, or is controlled at low cost.}
}
@entry{expectedmeansquares,
