@entry{selfsupervisedlearning,
name = {self supervised machine learning},
description = {Self supervised machine learning is a \gls{machinelearning} technique that trains models to create their own labels from \gls{unlabeleddata} data. This method is also known as predictive or pretext learning. It is useful because it allows models to be trained using large amounts of \gls{unlabeleddata} data, which is often cheaper and more abundant than \gls{labeleddata}. This makes SSL a cost-effective and time-efficient way to train models, especially in fields like computer vision and \gls{naturallanguageprocessing} where \gls{labeleddata} can be scarce. Self supervised learning creates its own \enquote{supervisory signals} by manipulating the \glspl{datum} itself to generate learning tasks, effectively acting like a supervised learning approach within an unsupervised framework.}
}
@entry{semisupervisedlearning,
