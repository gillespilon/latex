@entry{twodimarray,
name = {2d array},
description = {Two-dimensional array. A NumPy array whose \textit{.shape} has length 2. Often represents a matrix.}
}
@entry{aad,
name = {average absolute deviation},
description = {The average absolute deviation (AAD) measure does not square the distance from the average, so it is less affected by extreme \glspl{observation} than are the \gls{variance} and the \gls{standarddeviation}. $$\text{AAD} = \frac{\sum\limits_{i=1}^N\left(|Y_i - \overline{Y}|\right)}{N}$$}
}
@entry{anome,
name = {analysis of main effects},
description = {The analysis of main effects (ANOME) chart that compares the \gls{averagestatistic} of each level to the overall average},
}
@entry{anomr,
name = {analysis of mean ranges},
description = {The analysis of mean ranges (ANOMR) chart compares the average range of each level to the overall range},
}
@entry{cp,
name = {process capability index two-sided (rational sample)},
description = {The process capability index two-sided (rational sample) ($C_\text{p}$) compares the width of the process specification to the width of the process variation.},
}
@entry{cpk,
name = {process capability index one-sided (rational sample)},
description = {The process capability index one-sided (rational sample) ($C_\text{pk}$) calculates the distance from each specification limit to the \gls{averagestatistic}, compares them to the width of the process variation, and reports the smaller value.},
}
@entry{cpm,
name = {process capability index loss function (random sample)},
description = {The process capability index loss function (random sample) ($C_\text{pm}$) calculates process capability with respect to deviation from the \gls{averagestatistic} and deviation from the target.},
}
@entry{gui,
name = {graphical user interface},
plural = {graphical user interfaces},
description = {A graphical user interface (GUI) is a visual interface that allows users to interact with a computer or other electronic device using icons, buttons, and menus. GUIs are interactive and replace text-based commands with user-friendly actions. The goal of a GUI is to make decision points easy for users to find, understand, and use.}
}
@entry{icc,
name = {intraclass correlation coefficient},
description = {The intraclass correlation coefficient (ICC) describes how strongly units in the same group resemble each other, with and without operators.}
}
@entry{iqr,
name = {interquartile range},
description = {The interquartile range (IQR) is the value of the 75\textsuperscript{th} percentile minus the value of the 25\textsuperscript{th} percentile. This \gls{dispersionstatistic} attempts to measure the variability of points near the centre.},
}
@entry{knn,
name = {k-nearest neighbour},
description = {The k-nearest neighbour (k-NN) is a non-parametric supervised learning method used in classification and prediction. The input consists of the k closest training examples in a \gls{dataset}. The output depends on whether k-NN is used for classification or prediction.}
}
@entry{mad,
name = {median absolute deviation},
description = {The median absolute deviation (MAD) is a variation of the \gls{aad} that is even less affected by extremes in the tail because the \glspl{datum} in the tails have less influence on the calculation of the median than they do on the \gls{averagestatistic}. $$\text{MAD} = \text{median}\left(|Y_i - \tilde{Y}|\right)$$},
}
@entry{mae,
name = {mean absolute error},
description = {The mean absolute error (MAE) is the mean of the absolute value of the errors. $$\text{MAE} = \frac{\sum\limits_{i=1}^n\left(|y_i - \hat{y_i}|\right)}{n}$$}
}
@entry{msa,
name = {Measurement System Analysis},
description = {Measure system analysis (MSA) is a method to evaluate measurements systems, to understand the causes of measurement variation},
}
@entry{mse,
name = {mean squared error},
description = {The mean squared error (MSE) is the mean of the squared errors. $$\text{MSE} = \frac{\sum\limits_{i=1}^n\left(y_i - \hat{y_i}\right)^2}{n}$$}
}
@entry{pp,
name = {process capability index two-sided (random sample)},
description = {process capability index two-sided (random sample) ($P_\text{p}$) compares the width of the process specification to the width of the process variation},
}
@entry{ppk,
name = {process capability index one-sided (random sample)},
description = {The process capability index one-sided (random sample) ($P_\text{pk}$) calculates the distance from each specification limit to the \gls{averagestatistic}, compares them to the width of the process variation, and reports the smaller value.},
}
@entry{rmse,
name = {root-mean-squared error},
plural = {root-mean-squared errors},
description = {The root-mean-squared error (RMSE) is a frequently-used measure of the differences between values predicted by a model or an estimator and the values observed. $$\text{RMSE} = \sqrt{\frac{\sum\limits_{i=1}^{n} \left(y_i - \widehat{y}\right)^2}{n}}$$}
}
@entry{rpa,
name = {robotic process automation},
description = {Robotic process automation (RPA) uses software robots to handle repetitive computer tasks. The benefits include reduced execution time, customization for specific needs, perform complex workflows, and consistent repeatability to eliminate human errors.}
}
@entry{iam,
name = {identity and access management},
description = {Identity and access management (IAM) is a framework of policies and technologies to ensure that the right users have the appropriate access to technology resources. It identifies, authenticates, and controls access for individuals who will be using information technology resources, hardware, and access to applications.},
}
@entry{pam,
name = {privileged access management},
description = {Privileged access management (PAM) is a type of \gls{iam} and \gls{cybersecurity} technology that focuses on the control, monitoring, and protection of privileged accounts in an organization. Accounts with privileged access grant users enhanced permissions, making them prime targets for attackers due to their extensive access to vital systems and sensitive \glspl{datum}.}
}
@entry{xdr,
name = {extended detection and response},
description = {Extended detection and response (XDR) provides security incident detection and automated response capabilities for security infrastructure. It integrates threat intelligence and telemetry \glspl{datum} from multiple sources with security analytics to provide conceptualization and correlation of security alerts.}
}
@entry{abbreviation,
